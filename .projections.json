{
  "lib/*.ex": {
    "alternate": "test/{}_test.exs",
    "template": [
      "defmodule {capitalize|dot|camelcase} do",
      "  @moduledoc \"\"\"",
      "  \"\"\"",
      "  ",
      "end"
    ],
    "type": "lib"
  },

  "test/*_test.exs": {
    "alternate": "lib/{}.ex",
    "template": [
      "defmodule {capitalize|dot|camelcase}Test do",
      "  use ExUnit.Case",
      "  use ExUnitProperties",
      "",
      "  alias Euler.{capitalize|dot|camelcase}",
      "",
      "end"
    ],
    "type": "test"
  }
}
